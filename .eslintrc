{
  "extends": "airbnb-base",
  "rules": {
    "import/newline-after-import": 0,
    "no-confusing-arrow": ["error", { "allowParens": true }],
    "comma-dangle": ["error", "never"]
  },
  "plugins": ["jasmine", "mocha", "react"],
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false
  }
}
